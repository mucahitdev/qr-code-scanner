{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "components/*": ["src/components/*"],
      "screens/*": ["src/screens/*"],
      "helpers/*": ["src/helpers/*"],
      "assets/*": ["assets/*"],
      "store/*": ["src/store/*"],
      "common/*": ["src/common/*"],
      "navigations/*": ["src/navigations/*"],
    }
  },
  "exclude": ["node_modules"]
}